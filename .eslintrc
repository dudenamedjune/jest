{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jest": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/prefer-default-export": 0,
        "linebreak-style": 0,
        "react/require-default-props": 0
    },
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint"
}